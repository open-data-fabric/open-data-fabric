{
    "$id": "http://open-data-fabric.github.com/schemas/Watermark",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Represents a watermark in the event stream",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "systemTime",
        "eventTime"
    ],
    "properties": {
        "systemTime": {
            "type": "string",
            "format": "date-time"
        },
        "eventTime": {
            "type": "string",
            "format": "date-time"
        }
    }
}