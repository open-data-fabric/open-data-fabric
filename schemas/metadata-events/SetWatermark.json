{
  "$id": "http://open-data-fabric.github.com/schemas/SetWatermark",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Indicates the advancement of the dataset's watermark.",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "outputWatermark"
  ],
  "properties": {
    "outputWatermark": {
      "type": "string",
      "format": "date-time",
      "description": "Last watermark of the output data stream."
    }
  }
}