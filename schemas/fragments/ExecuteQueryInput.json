{
  "$id": "http://open-data-fabric.github.com/schemas/ExecuteQueryInput",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Describes a slice of input data used during a transformation",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "datasetId"
  ],
  "properties": {
    "datasetId": {
      "type": "string",
      "format": "dataset-id",
      "description": "Input dataset identifier."
    },
    "blockInterval": {
      "$ref": "/schemas/BlockInterval",
      "description": "Blocks that went into this transaction."
    },
    "offsetInterval": {
      "$ref": "/schemas/OffsetInterval",
      "description": "Data range that went into this transaction."
    }
  }
}